        spacing: 10

        BoxLayout:
            TextInput:
                id: entry
                font_size: 32
                multiline: False

        BoxLayout:
            Label:
                font_size: 32
                text: "Press T and Y to traverse. Press R to select vehicle desired."

        BoxLayout:
            Label:
                font_size: 32
                text: "When R is depressed, the vehicle id will be shown."

        BoxLayout:
            spacing: 10
            VehGal:
                text: "Enter Gallery"
                on_press:
                    app.openVehicleGallery()
